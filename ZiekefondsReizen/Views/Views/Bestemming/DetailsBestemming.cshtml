@model BestemmingDetailsViewModel
@{
    ViewData["Title"] = "Bestemming Details";
}

<h2>@Model.Naam</h2>
<br />
<table class="table">
    <tr>
        <td>ID</td>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <td>Code</td>
        <td>@Model.Code</td>
    </tr>
    <tr>
        <td>Minimum Leeftijd</td>
        <td>@Model.MinLeeftijd jaar oud</td>
    </tr>
    <tr>
        <td>Maximum Leeftijd</td>
        <td>@Model.MaxLeeftijd jaar oud</td>
    </tr>
    <tr>
        <td>Beschrijving</td>
        <td>@Model.Beschrijving</td>
    </tr>
</table>

<br />

<a asp-action="Index" class="btn btn-primary">Terug naar de lijst</a>